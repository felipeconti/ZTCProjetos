<Alloy>
	<Window id="projectsWindow">
		<RightNavButton platform="ios">
			<Button id="refreshButton" onClick="loadProjects">
		</RightNavButton>
		<Require src="ActionBar" id="actionBar" platform='android'/>
		<TableView id="projectsTableView" dataCollection="projects">
			<SearchBar hintText="Procurar"></SearchBar>
			<TableViewRow title="{name}" hasChild="true">
		</TableView>
	</Window>
</Alloy>